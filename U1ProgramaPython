cadena=["Diana","Maritza","Bartolome","Rodriguez"]
cadenadescompuesta=[]
cadenafinal=[]

print("Se mostrara la cadena descompuesta")
archivo=open("datos.txt","r")

for linea in archivo.readlines():
    cadenadescompuesta.append(linea)
for item1 in cadena:
    for item2 in cadenadescompuesta:
        if item1 in item2:
            cadenafinal.append(item1)
print(cadenafinal)
